{
	"name": "helloworld",
	"displayName": "HelloWorld",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.65.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:helloworld.helloWorld"
	],
	"browser": "./dist/web/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "log-msg",
					"title": "Click me",
					"icon": "media/icon1.svg"
				}
			]
		},
		"views": {
			"log-msg": [
				{
					"id": "test",
					"name": "test",
					"icon": "media/icon1.svg",
					"contextualTitle": "Click me"
				},
				{
					"id": "commands",
					"name": "Commands",
					"icon": "media/icon1.svg",
					"contextualTitle": "commands for extension"
				}
			]
		},
		"viewsWelcome": [
			{
				"view": "test",
				"contents": "Test button.\n[Click me](command:test.refreshEntry)"
			},
			{
				"view": "commands",
				"contents": "[Start extension](command:helloworld.helloWorld)\n[Build](command:extension.build)\n[Run](command:extension.run)\n[Stop](command:extension.stop)"
			}
		],
		"commands": [
			{
				"command": "test.refreshEntry",
				"title": "Refresh"
			},
			{
				"command": "helloworld.helloWorld",
				"title": "Start extensions Hello World"
			},
			{
				"command": "extension.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "extension.build",
				"title": "Build"
			},
			{
				"command": "extension.run",
				"title": "Run"
			},
			{
				"command": "extension.stop",
				"title": "Stop"
			}
		]
	},
	"scripts": {
		"test": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. --extensionTestsPath=dist/web/test/suite/index.js",
		"pretest": "npm run compile-web",
		"vscode:prepublish": "npm run package-web",
		"compile-web": "webpack",
		"watch-web": "webpack --watch",
		"package-web": "webpack --mode production --devtool hidden-source-map",
		"lint": "eslint src --ext ts",
		"run-in-browser": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. ."
	},
	"devDependencies": {
		"@types/mocha": "^9.1.0",
		"@types/vscode": "^1.65.0",
		"os-browserify": "^0.3.0",
		"@types/webpack-env": "^1.16.3",
		"@typescript-eslint/eslint-plugin": "^5.12.1",
		"@typescript-eslint/parser": "^5.12.1",
		"@vscode/test-web": "^0.0.22",
		"assert": "^2.0.0",
		"eslint": "^8.9.0",
		"mocha": "^9.2.1",
		"process": "^0.11.10",
		"ts-loader": "^9.2.6",
		"typescript": "^4.5.5",
		"webpack": "^5.69.1",
		"webpack-cli": "^4.9.2",
		"webpack-dev-server": "^4.8.1"
	}
}
